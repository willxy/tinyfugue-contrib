Is there a faster, more efficient way to sort a group of numbers, e.g. from
least to greatest, than the quicksort routine below?

;; a quicksort routine
;; usage: /qsort <comparison-function> <list>

;; for least to greatest I would use this as the comparison-function
/def diff = /return %{1} - %{2}

/def qsort = \
    /if ({#} <= 2) \
    /_echo %{-1} %; \
    /else \
    /let compare=%{1} %;\
    /shift %;\
/let key=%{R} %;\
;/let key=%{1} %;\
    /let same=%;\
    /let small=%;\
    /let large=%;\
    /let diff=%;\
        /while ({#}) \
            /test diff:=%compare({1}, key) %;\
                /if (!diff) \
                /let same=%same %1 %; \
                /elseif (diff < 0) \
                /let small=%small %1 %; \
                /else \
                /let large=%large %1 %; \
                /endif %; \
            /shift %; \
        /done %; \
/_echo $(/qsort %compare %small) %same $(/qsort %compare %large) %; \
/endif

