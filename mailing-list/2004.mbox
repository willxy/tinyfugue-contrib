;; Set up status bar. Make it look more like emacs.

/set status_attr=r
; This doesn't seem to work.
/set status_pad=-

;; Add status bar fields to track HP, FP, GP, and XP

/status_add -B xp:9
/status_add -B gp:7
/status_add -B fp:5
/status_add -B hp:5

/set status_var_hp pad("H:", 2, hp, 3)
/set status_var_fp pad("F:", 2, fp, 3)
/set status_var_gp pad("G:", 2, gp, 5)
/set status_var_xp pad("X:", 2, xp, 7)

;; Return a color based on the delta of the arguments.
;; This retruns background colors because the status
;; bar is in reverse video. So backgrounds become
;; foregrounds and vice versa.

/def hilite_change = \
  /if ({1} > {2}) \
    /result "BCgreen"%; \
  /elseif ({1} < {2}) \
    /result "BCred"%; \
  /else \
    /result "n"%; \
  /endif

;; Macro to track HP/FP/GP/XP and to hilight changes
;; in them.

/def hgfx_status = \
  /set status_attr_var_hp $(/hilite_change %{1} %{hp})%; \
  /set status_attr_var_fp $(/hilite_change %{2} %{fp})%; \
  /set status_attr_var_gp $(/hilite_change %{3} %{gp})%; \
  /set status_attr_var_xp $(/hilite_change %{4} %{xp})%; \
  /set hp=%{1}%; \
  /set fp=%{2}%; \
  /set gp=%{3}%; \
  /set xp=%{4}

;; Gather HP/FP/GP/XP info from prompt or hfrep information,
;; and set them into the stauts bar, appropriately highlighted.
;; (These are all on one line in my .tfrc)

/def -mregexp -h'PROMPT ^H:[^0-9]*([0-9]+)[^0-9]* F:([0-9]+) G:([0-9]+) X:
([0-9]+)> ' hfgx_hook = /hgfx_status %P1 %P2 %P3 %P4 %; /test prompt({*})
/def -mregexp -t"^\[Hp [^0-9]*([0-9]+)[^0-9]*  Fp ([0-9]+)" hfgx_trig 
= /hgfx_status %P1 %P2 %{gp} %{xp}
