;empty element.
;Get_array2_count will count the number of elements starting at index
;from "start"
;--------------------------------------------------------------------------
;/def get_array_count = \
;  /test GA_Name := {1} %;\
;  /test GA_Count := {2} - 1 %;\
;  /test GA_Element := " " %;\
;  /while (strlen(GA_Element) > 0) \
;    /test ++GA_Count %;\
;    /test GA_Element := get_array(GA_Name, GA_Count) %;\
