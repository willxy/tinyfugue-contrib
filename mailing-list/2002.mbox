@@ -0,0 +1,426 @@
+/*
+ * Client decompression module for the mud client compression protocol.
+ * See http://homepages.ihug.co.nz/~icecube/compress/ for more details.
+ *
+ * mccpDecompress.c - module code. Link this with your client code.
+ *
+ * Oliver Jowett <icecube$ihug.co.nz>. Demangle address as needed.
+ *
+ * This code is placed in the public domain.
+ *
+ */
+
+/* Modified: 20000530 */
+
+/* See mccp.h for API information */
+
+#include <stdlib.h>
+#include <stdio.h>
+#include <string.h>
+
+#include "config.h"
+#ifdef MUDCOMPRESS
+
+#include <zlib.h>
+
+#include "mccp.h"
+
+/* Telnet values we're interested in */
+
+#define IAC             255
+#define DONT            254
+#define DO              253
+#define WONT            252
+#define WILL            251
+#define SB              250
+#define SE              240
+
+#define TELOPT_COMPRESS 85
+#define TELOPT_COMPRESS2 86
+
+/* We say DO COMPRESS2 to WILL COMPRESS2, then DONT COMPRESS to WILL COMPR=
ESS
+ * -or-
+ * We say DO COMPRESS to WILL COMPRESS if it arrives before any COMPRESS2.
+ *
+ * Later the server sends IAC SB COMPRESS IAC SE (v2) or IAC SB COMPRESS W=
ILL
+ * SE (v1), and immediately following
+ * that, begins compressing
+ *
+ * Compression ends on a Z_STREAM_END, no other marker is used
+ */
+
+static char will_v1[] =3D { IAC, WILL, TELOPT_COMPRESS, 0 };
+static char do_v1[] =3D   { IAC, DO,   TELOPT_COMPRESS, 0 };
+static char dont_v1[] =3D { IAC, DONT, TELOPT_COMPRESS, 0 };
+static char on_v1[] =3D   { IAC, SB, TELOPT_COMPRESS, WILL, SE, 0 };
+
+static char will_v2[] =3D { IAC, WILL, TELOPT_COMPRESS2, 0 };
+static char do_v2[] =3D   { IAC, DO,   TELOPT_COMPRESS2, 0 };
+static char on_v2[] =3D   { IAC, SB, TELOPT_COMPRESS2, IAC, SE, 0 };
+
+/* "Opaque" state object */
+
+struct mc_state_s {
+    z_stream *stream;      /* stream we're using */
+
+    unsigned char *inbuf;  /* input buffer (data from mud) */
+    unsigned int insize;   /* .. and how much is used */
+    unsigned int inalloc;  /* .. and how much is allocated */
+
+    unsigned char *outbuf; /* output buffer (data to user) */
+    unsigned int outsize;  /* .. and how much is used */
+    unsigned int outalloc; /* .. and how much is allocated */
+
+    int error;
+    int resp_v1;  /* waiting to send IAC DO/DONT COMPRESS */
+    int resp_v2;  /* waiting to send IAC DO COMPRESS2 */
+    int got_v2;   /* responded to a IAC WILL COMPRESS2 already */
+
+    unsigned long comp;
+    unsigned long uncomp;
+};
+
+/* Initialise a new state object */
+mc_state *mudcompress_new(void)
+{
+    mc_state *state;
+
+    state =3D malloc(sizeof(*state));
+    state->stream   =3D NULL;   /* Not decompressing */
+    state->inalloc  =3D 2048;
+    state->outalloc =3D 2048;
+    state->inbuf    =3D malloc(state->inalloc);
+    state->outbuf   =3D malloc(state->outalloc);
+    state->insize   =3D 0;
+    state->outsize  =3D 0;
+    state->error    =3D 0;
+    state->comp     =3D 0;
+    state->uncomp   =3D 0;
+    state->resp_v1  =3D 0;
+    state->resp_v2  =3D 0;
+    state->got_v2   =3D 0;
+
+    return state;
+}
+
+/* Clean up a state object */
+void mudcompress_delete(mc_state *state)
+{
+    if (state->stream) {
+        inflateEnd(state->stream);
+        free(state->stream);
+    }
+
+    free(state->inbuf);
+    free(state->outbuf);
+    free(state);
+}
+
+/* zlib helpers */
+
+static void *zlib_alloc(void *opaque, unsigned int items, unsigned int siz=
e)
+{
+    return calloc(items, size);
+}
+
+static void zlib_free(void *opaque, void *address)
+{
+    free(address);
+}
+
+static void grow_inbuf(mc_state *state, int needed)
+{
+    int old =3D state->inalloc;
+
+    while (state->inalloc < state->insize + needed)
+        state->inalloc *=3D 2;
+
+    if (old !=3D state->inalloc)
+        state->inbuf =3D realloc(state->inbuf, state->inalloc);
+}
+
+static void grow_outbuf(mc_state *state, int needed)
+{
+    int old =3D state->outalloc;
+
+    while (state->outalloc < state->outsize + needed)
+        state->outalloc *=3D 2;
+
+    if (old !=3D state->outalloc)
+        state->outbuf =3D realloc(state->outbuf, state->outalloc);
+}
+
+static void decompress_inbuf(mc_state *state)
+{
+    int status;
+
+    /* We are now decompressing from inbuf to outbuf */
+
+    if (!state->insize)
+        return; /* nothing to decompress? */
+
+    state->stream->next_in =3D state->inbuf;
+    state->stream->next_out =3D state->outbuf + state->outsize;
+    state->stream->avail_in =3D state->insize;
+    state->stream->avail_out =3D state->outalloc - state->outsize;
+
+    status =3D inflate(state->stream, Z_PARTIAL_FLUSH);
+
+    if (status =3D=3D Z_OK || status =3D=3D Z_STREAM_END) {
+        /* Successful decompression */
+
+        /* Remove used data from inbuf */
+        state->comp +=3D state->insize - state->stream->avail_in;
+        state->uncomp +=3D state->stream->next_out - state->outbuf;
+
+        memmove(state->inbuf, state->stream->next_in, state->stream->avail=
_in);
+        state->insize =3D state->stream->avail_in;
+
+        /* Update outbuf pointers */
+        state->outsize =3D state->stream->next_out - state->outbuf;
+
+        /* Done */
+
+        if (status =3D=3D Z_STREAM_END) {
+            /* Turn off compression too */
+
+            grow_outbuf(state, state->insize);
+
+            memcpy(state->outbuf + state->outsize, state->inbuf, state->in=
size);
+            state->outsize +=3D state->insize;
+            state->insize =3D 0;
+
+            inflateEnd(state->stream);
+            free(state->stream);
+            state->stream =3D NULL;
+        }
+
+        return;
+    }
+
+    if (status =3D=3D Z_BUF_ERROR) {
+        /* Full buffers? Maybe we need more output space.. */
+
+        if (state->outsize * 2 > state->outalloc) {
+            grow_outbuf(state, state->outalloc);
+            decompress_inbuf(state);
+        }
+
+        return;
+    }
+
+    /* Error */
+    state->error =3D 1;
+}
+
+/* We received some data */
+void mudcompress_receive(mc_state *state, const char *data, unsigned len)
+{
+    int i;
+
+    if (state->error)
+        return;
+
+    if (!state->stream) {
+        int residual =3D -1;
+        int clen;
+
+        /* Just copy to outbuf. Also copy any residual inbuf */
+
+        grow_outbuf(state, len + state->insize);
+        memcpy(state->outbuf + state->outsize, data, len);
+        state->outsize +=3D len;
+        memcpy(state->outbuf + state->outsize, state->inbuf, state->insize=
);
+        state->outsize +=3D state->insize;
+        state->insize =3D 0;
+
+        /* Check for Magic Marker. ugh this is messy */
+        for (i=3D0; i < state->outsize; i++) {
+            if (state->outbuf[i] =3D=3D IAC) {
+                if (i + 1 < state->outsize && state->outbuf[i+1] =3D=3D IA=
C) {
+                    /* IAC IAC - ignore */
+                    i++;
+                    continue;
+                }
+
+                clen =3D (i + strlen(will_v1) < state->outsize) ? strlen(w=
ill_v1) : state->outsize - i;
+
+                if (!memcmp(&state->outbuf[i], will_v1, clen)) {
+                    if (clen !=3D strlen(will_v1)) {
+                        /* Partial match. Save it. */
+                        residual =3D i;
+                        break;
+                    }
+
+                    /* If we got WILL COMPRESS2 then refuse COMPRESS, othe=
rwise
+                     * accept it
+                     */
+
+                    if (state->got_v2)
+                        state->resp_v1 =3D -1;
+                    else
+                        state->resp_v1 =3D 1;
+
+                    memmove(&state->outbuf[i],
+                            &state->outbuf[i + strlen(will_v1)],
+                            state->outsize - strlen(will_v1));
+                    state->outsize -=3D strlen(will_v1);
+                    i--;
+                    continue;
+                }
+
+                if (!memcmp(&state->outbuf[i], will_v2, clen)) {
+                    if (clen !=3D strlen(will_v2)) {
+                        /* Partial match. Save it. */
+                        residual =3D i;
+                        break;
+                    }
+
+                    state->resp_v2 =3D 1;
+                    state->got_v2 =3D 1;
+
+                    memmove(&state->outbuf[i],
+                            &state->outbuf[i + strlen(will_v2)],
+                            state->outsize - strlen(will_v2));
+                    state->outsize -=3D strlen(will_v2);
+                    i--;
+                    continue;
+                }
+
+                clen =3D (i + strlen(on_v1) < state->outsize) ? strlen(on_=
v1) : state->outsize - i;
+
+                if ((!memcmp(&state->outbuf[i], on_v1, clen) && !state->go=
t_v2) ||
+                    (!memcmp(&state->outbuf[i], on_v2, clen) && state->got=
_v2)) {
+                    if (clen !=3D strlen(on_v1)) {
+                        /* Partial match. Save it. */
+                        residual =3D i;
+                        break;
+                    }
+
+                    /* Switch to compression */
+                    /* copy any compressible bits to our inbuf */
+
+                    grow_inbuf(state, state->outsize - i - strlen(on_v1));
+
+                    memcpy(state->inbuf,
+                           state->outbuf + i + strlen(on_v1),
+                           state->outsize - i - strlen(on_v1));
+
+                    state->insize =3D state->outsize - i - strlen(on_v1);
+
+                    /* clean up our output buffer */
+                    state->outsize =3D i;
+
+                    /* init stream */
+                    state->stream =3D malloc(sizeof(z_stream));
+                    state->stream->zalloc =3D zlib_alloc;
+                    state->stream->zfree =3D zlib_free;
+                    state->stream->opaque =3D NULL;
+
+                    if (inflateInit(state->stream) !=3D Z_OK) {
+                        state->error =3D 1;
+                        free(state->stream);
+                        state->stream =3D NULL;
+                        return;
+                    }
+
+                    /* Continue with decompression */
+                    break;
+                }
+            }
+        }
+
+        if (!state->stream) { /* didn't start decompressing? */
+            /* We might have some residual, copy to inbuf for later checki=
ng */
+
+            if (residual !=3D -1) {
+                grow_inbuf(state, state->outsize - residual);
+                memcpy(state->inbuf + state->insize, state->outbuf + resid=
ual, state->outsize - residual);
+                state->outsize =3D residual;
+            }
+
+            return;
+        }
+    } else {
+        /* New data to decompress. Copy to inbuf */
+        grow_inbuf(state, len);
+        memcpy(state->inbuf + state->insize, data, len);
+        state->insize +=3D len;
+    }
+
+    decompress_inbuf(state);
+}
+
+/* How much data is available? */
+int mudcompress_pending(mc_state *state)
+{
+    return state->error ? 0 : state->outsize;
+}
+
+/* Was there an error? */
+int mudcompress_error(mc_state *state)
+{
+    return state->error;
+}
+
+/* Get some data */
+int mudcompress_get(mc_state *state, char *buf, int size)
+{
+    int copied;
+
+    if (state->error || !state->outsize)
+        return 0;
+
+    if (size > state->outsize)
+        copied =3D state->outsize;
+    else
+        copied =3D size;
+
+    memcpy(buf, state->outbuf, copied);
+    state->outsize -=3D copied;
+    if (state->outsize)
+        memmove(state->outbuf, state->outbuf + copied, state->outsize);
+
+    /* Do some more decompression */
+    decompress_inbuf(state);
+
+    return copied;
+}
+
+void mudcompress_stats(mc_state *state, unsigned long *comp, unsigned long=
 *uncomp)
+{
+    *comp =3D state->comp;
+    *uncomp =3D state->uncomp;
+}
+
+const char *mudcompress_response(mc_state *state)
+{
+    if (state->resp_v1 =3D=3D 1) {
+        state->resp_v1 =3D 0;
+        return do_v1;
+    }
+
+    if (state->resp_v1 =3D=3D -1) {
+        state->resp_v1 =3D 0;
+        return dont_v1;
+    }
+
+    if (state->resp_v2) {
+        state->resp_v2 =3D 0;
+        return do_v2;
+    }
+
+    return NULL;
+}
+
+int mudcompress_compressing(mc_state *state)
+{
+    return (state->stream !=3D NULL);
+}
+
+int mudcompress_v2(mc_state *state)
+{
+    return (state->stream !=3D NULL && state->got_v2);
+}
+
