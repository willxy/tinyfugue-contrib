;LineA = (A_X1, A_Y1, A_X2, A_Y2)
;LineB = (B_X1, B_Y1, B_X2, B_Y2)
;SlopeA = (A_Y2 - A_Y1) / (A_X2 - A_X1)
;SlopeB = (B_Y2 - B_Y1) / (B_X2 - B_Y1)
;CA = A_Y1 - SlopeA * A_X1
;CB = B_Y2 - SlopeB * B_X2
;JoinX = (CB - CA) / (SlopeA - SlopeB)
;JoinY = (SlopeA * JoinX + CA)

/def FindJoin1 = \
  /test A_X1 := %1 %;\
  /test A_Y1 := %2 %;\
  /test A_X2 := %3 %;\
  /test A_Y2 := %4 %;\
  /test B_X1 := %5 %;\
  /test B_Y1 := %6 %;\
  /test B_X2 := %7 %;\
  /test B_Y2 := %8 %;\
  /test SlopeA := (A_Y2 - A_Y1) / (A_X2 - A_X1) %;\
  /test SlopeB := (B_Y2 - B_Y1) / (B_X2 - B_X1) %;\
  /test CA := A_Y1 - SlopeA * A_X1 %;\
  /test CB := B_Y2 - SlopeB * B_X2 %;\
  /test JoinX := (CB - CA) / (SlopeA - SlopeB) %;\
  /test JoinY := (SlopeA * JoinX + CA) %;\
  /test echo(strcat('Joining location = ', JoinX, ',', JoinY))

