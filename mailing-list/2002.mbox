;------------------------------------------------------------------------
;Returns the string value of any variable that contains an index number.
;var_index(variable, index), variable must be inclosed in quotes.
;------------------------------------------------------------------------
/def var_index = \
  /def var_index2 = \
    /return %1%2%;\
  /test var_indexSt := var_index2()%;\
  /undef var_index2%;\
  /return var_indexSt%;\

;------------------------------------------------------------------------
;Replaces <locator> found in string with what ever is contained in <name>
;if there is no <locator> in the string then the name is not added and
;just the message is returned.
;string_replace(message, "..", name) : ".." is the locator.
;------------------------------------------------------------------------
/def string_replace = \
  /test string_replaceSt := {1}%;\
  /test string_replaceI := strstr(string_replaceSt, {2})%;\
  /IF (string_replaceI > -1) \
    /test string_replaceSt := strcat( substr(string_replaceSt, 0, string_replaceI), {3}, substr(string_replaceSt, string_replaceI + 2, 255) )%;\
  /ENDIF%;\
  /RETURN string_replaceSt

;---------------------------------------------------------------------------------------
;returns a chat message command with message
;port_message(type, name, gender, index) :1 type = type of message IE port_good, port_no, etc
;                                         2 name = name to be used in port message.
;                                         3 gender = only appears at spots marked with "&&"
;                                         4 index = which message of type to return.
;---------------------------------------------------------------------------------------
/def port_message = \
  /test port_messageSt := string_replace( var_index({1}, {4} ), "..", {2} )%;\
  /test port_messageSt := string_replace( var_index({1}, {4} ), "..", {2} )%;\
  /test PM_G := {3} %;\
  /IF (PM_G =~ "Male") \
    /test PM_G := "he" %;\
  /ELSE \
    /IF (PM_G =~ "Female") \
      /test PM_G := "she" %;\
    /ELSE \
      /test PM_G := "it" %;\
    /ENDIF%;\
  /ENDIF%;\
  /test port_messageSt := string_replace( port_messageSt , "&&", PM_G )%;\
  /test port_messageSt := string_replace( port_messageSt , "&&", PM_G )%;\
  /IF (PM_G =~ "he") \
    /test PM_G := "him" %;\
  /ELSE \
    /IF (PM_G =~ "she") \
      /test PM_G := "her" %;\
    /ENDIF%;\
  /ENDIF%;\
  /test port_messageSt := string_replace( port_messageSt , "%%", PM_G )%;\
  /test port_messageSt := string_replace( port_messageSt , "%%", PM_G )%;\
  /RETURN port_messageSt


