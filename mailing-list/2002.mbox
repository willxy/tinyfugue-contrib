/def getstatxp = /return statval%1

/def statxp1 = \
  /test expstat := {1}%;\
  /test expstatC := 0%;\
  /test exptotal := 0%;\
  /while (expstatC < expstat) \
    /test expstatC := expstatC + 1%;\
    /IF ( (getstatxp(expstatC -1) = 0) & (expstatC > 1)) \
      /test echo("Exp cost may be inaccurate due to insufficent data")%;\
    /ENDIF%;\
    /test exptotal := exptotal + getstatxp(expstatC - 1)%;\
  /DONE%;\
  /return exptotal

;statxp -<num> returns exp cost of stat num
;statxp <num> returns total cost of exp from 1 to num all together.
;USAGE:
;statxp -43 would return how much exp needed to stat 44
;statxp 43 would return total exp it would cost to get to stat 43

/def statxp = \
  /IF ({1} < 0) \
    /test echo(strcat("Current exp cost to raise to next stat after ",abs({1})," = ",getstatxp(abs({1}))))%;\
    /return getstatxp(abs({1}))%;\
  /ELSE \
    /test echo(strcat("Total exp cost to raise stat to ",{1}," = ",statxp1({1})))%;\
    /return statxp1({1})%;\
  /ENDIF

;statxp2
;list all total cost of all your stats, this may cause an overflow error.

/def statxp2 = \
  /test echo(statxp1({1})+statxp1({2})+statxp1({3})+statxp1({4})+statxp1({5})+statxp1({6}))

;statxplist %1 %2 lists all exp values from stat %1 to stat %2
;USAGE
;statxplist 12 30 would list all stats from 12 ending at 30 with how much
;                  exp it would cost to get to next stat.

/def statxplist = \
  /for expstatC2 %1 %2 /test echo(strcat(expstatC2," : ",getstatxp(expstatC2)))

