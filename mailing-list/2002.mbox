/eval /set LIGHT_GREEN   $[char(27)][1;32m
/eval /set LIGHT_MAGENTA $[char(27)][1;35m
/eval /set LIGHT_RED     $[char(27)][1;31m
/eval /set MAGENTA       $[char(27)][0;35m
/eval /set NORMAL        $[char(27)][0m
/eval /set RED           $[char(27)][0;31m
/eval /set REVERSE       $[char(27)][7m
/eval /set UNDERSCORE    $[char(27)][4m
/eval /set WHITE         $[char(27)][1;37m
