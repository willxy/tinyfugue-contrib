----- autotick.tf -----
;;;; Tick counting
;;;; Useful on Diku muds.  To use, just /load this file and add
;;;; something like tick:2 at your status_fields variable.
;;;; The macro tick_action may be changed or undefined.
;;;; You may need edit the command used to toggle mud autotick info.
;;;; And the mud tick info pattern "--> TICK <--".

;;;; usage:
;;; /tick       Toggles tick counting ON/OFF.
;;; /tick <n>   Set the tick length to <n> seconds (default is 30).

/loaded autotick.tf

/set tick=
/set ticksize=30
/set autotick=autotick
/def -i tick_action = /echo % TICK

/def -i tick = \
    /if ({1}) \
        /set ticksize=%1%;\
    /elseif (tick) \
        /@test _tickpid & (kill(_tickpid), unset(_tickpid))%;\
        /undef ~tick%;\
        /set tick=%;\
        /send %autotick%;\
        /echo -e %% Auto-Tick OFF%;\
    /else \
        /set tick=%ticksize%;\
        /def -i -ag -mglob -t"--> TICK <--" ~tick = \
            /set tick=%%ticksize%%;\
            /if (ismacro("tick_action")) /tick_action%%; /endif%;\
        /repeat -1 %maxpri /set tick=$$[tick - 1]%;\
        /set _tickpid=%?%;\
        /send %autotick%;\
        /echo -e %% Auto-Tick ON%;\
    /endif




