;;; you may also use this as an simple key-value hash list
;;; e.g. /set_array weapon s1 sword_with_a_very_long_id
;;;
;;; /def wield = /get_array weapon %1%;wield %?
;;;
;;; usage:
;;; instead of: wield sword_with_a_very_long_id
;;; just use: /wield s1

;------------------------------------------------------------------------
;Get array : An artificial array function to simulate a real array usage:
;get_array("Array name here", I)
;example get_array("Dir_Array", 43) returns the 43rd element from "Dir_Array"
;------------------------------------------------------------------------

/def get_array = /return _%1_array_%2

;------------------------------------------------------------------------
;PUT array : An artificial array function to simulate a real array usage:
;put_array("Array name here", I, st)
;example put_array("Dir_Array", 43, "sw") puts "sw" at element 43 in 
"Dir_Array"
;------------------------------------------------------------------------

/def put_array = /if ({#}2) /set _%1_array_%2=%-3%; \
         /else /unset _%1_array_%2%; /endif

;------------------------------------------------------------------------
;PURGE array : Purges an artificial array made by get_array & put_array usage:
;purge_array("Array name here", number of elements)
;example purge_array("Dir_Array", 100) purges 100 elements of "Dir_Array"
;NOTE: Purge array starts from element 0
;------------------------------------------------------------------------

/def purge_array = /quote -S /unset `/listvar -s _%1_array_*

/def list_array = /listvar _%1_array_*

