/eval /set BOLD          $[char(27)][1m
/eval /set BROWN         $[char(27)][0;33m
/eval /set CYAN          $[char(27)][0;36m
/eval /set GRAY          $[char(27)][0;37m
/eval /set GREEN         $[char(27)][0;32m
/eval /set GREY          $[char(27)][0;37m
/eval /set LIGHT_BLACK   $[char(27)][1;30m
/eval /set LIGHT_BLUE    $[char(27)][1;34m
