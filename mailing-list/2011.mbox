&nbsp;&nbsp;&nbsp; if table.getn(fields) &lt; 2 or fields[1]:len() &lt; 1 or fields[2]:len() &lt; 1 then
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; return
&nbsp;&nbsp;&nbsp; end 
&nbsp;&nbsp;&nbsp; short = mescape(fields[1])
&nbsp;&nbsp;&nbsp; long = mescape(fields[2])
--&nbsp;&nbsp;&nbsp; tf_eval(&quot;/repeat -0.03 1&nbsp; /echo S,L [&quot; .. short .. &quot;][&quot; .. long ..&quot;]&quot;)
&nbsp;&nbsp;&nbsp; item_id = select_single(string.format(&quot;SELECT item_id FROM t_item WHERE short = %s AND descr = %s&quot;, short, long))
&nbsp;&nbsp;&nbsp; if not item_id then
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; assert (conn:execute(string.format(&quot;INSERT INTO t_item VALUES ('', %s, %s, NOW())&quot;, short, long)))
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; item_id = select_single(&quot;SELECT LAST_INSERT_ID()&quot;)
&nbsp;&nbsp;&nbsp; end
&nbsp;&nbsp;&nbsp; <FONT COLOR="#ff0000">echo(item_id)</FONT>
end 

function item_search(str, t)
&nbsp;&nbsp;&nbsp; mstr = conn:escape(str)
&nbsp;&nbsp;&nbsp; query = &quot;SELECT item_id, short, descr, added FROM t_item WHERE short LIKE '%&quot; .. mstr .. &quot;%' ORDER BY short&quot;;
&nbsp;&nbsp;&nbsp; cur = assert(conn:execute(query))
&nbsp;&nbsp;&nbsp; row = cur:fetch ({}, &quot;a&quot;)
&nbsp;&nbsp;&nbsp; while row do
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; short = capitalize(row.short)
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; msg(string.format(&quot;@{B}%s @{nCwhite}[%s]&quot;, short, row.added))
&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; row = cur:fetch (row, &quot;a&quot;)
&nbsp;&nbsp;&nbsp; end
&nbsp;&nbsp;&nbsp; msg('Found @{B}' .. cur:numrows() .. '@{n} items!!')
&nbsp;&nbsp;&nbsp; cur:close()
end

