As part of my efforts to collect useful TF snippets that have come
before me, I found this:

;; Written by Illiad: posted to Aetolia: The Midnight Age forum
;;
http://forums.aetolia.com/index.php?showtopic=1843&st=480&p=243150&#entry243150 

<http://forums.aetolia.com/index.php?showtopic=1843&st=480&p=243150&#entry243150>

Usage:

/cache -h -a<string> -d<string> -n<integer> -csrepx
-------------------------------------------------------------------------------------

Options:

-h      Prints the help file you're currently viewing.
-n      Set the number of times you'd like a command added to the cache
via -a.
-a      string, (which must be quoted), to add to the stack.
-d      part of command in stack, (which also must be quoted), to delete
from the
        stack.
-c      Using the switch will clear the stack.
-s      Halts the cache temporarily.
-r      Resumes the cache after a halt.
-e      Execute next command in the stack.
-p      Print cache.  (Still under development.)
-x      Sets cache as active, meaning commands stored in the stack will
automaticly
        be executed as soon as possible.  Use this switch again to set
it to inactive.


CODE


/setenv _fstack=
/setenv _fbuffer=

/def _add_cache = \
    /let _add_stack=%{*}%;\
    /let _f_return=%;\
    /if (_fstack =~ '') \
        /set _fstack=|%{_add_stack}|%;\
        /let _f_return=%?%;\
    /else \
        /test _fstack := strcat(tolower(_fstack), "|", _add_stack, "|")%;\
        /if (_fstack !~ '') /let _f_return=1%;/endif%;\
    /endif%;\
    /test _fstack := replace("||", "|", _fstack)%;\
    /if (_fstack =~ '|') /setenv _fstack=%;/endif

/def _halt_cache = \
    /if (_fstack !~ '') \
        /echo -waetolia *-- Halting Cache. --*%;\
        /echo -waetolia *-- Use "/cache -r" to start it again. --*%;\
        /setenv _fbuffer=%{_fstack}%;\
        /setenv _fstack=%;\
    /else \
        /echo -waetolia *-- Nothing to halt, cache is already empty. --*%;\
    /endif

/def _res_cache = \
    /if (_fbuffer !~ '') \
        /echo -waetolia *-- Resuming cache operation --*%;\
        /setenv _fstack=%{_fbuffer}%;\
        /setenv _fbuffer=%;\
        /return%;\
    /endif%;\
    /_echo *-- Cache isn't halted --*

/def _rm_cache = \
    /let _searching=1%;\
    /let _fc_rm_string=%{*}%;\
    /let _fc_rm_string=$[tolower(_fc_rm_string)]%;\
    /echo -waetolia *-- Attempting to delete %{_fc_rm_string} --*%;\
    /test regmatch(strcat("(\\|", _fc_rm_string, "[^\\|]*)"), _fstack)%;\
    /while ( (%{P1} !~ '') & (_searching !~ '')) \
        /test regmatch(strcat("(\\|", _fc_rm_string, "[^\\|]*)"),
_fstack)%;\
        /let searching=%{P1}%;\
        /if ( (%{PL} !~ '') | (%{PR} !~ '') ) \
            /setenv _fstack=%{PL}%{PR}%;\
            /test _fstack := replace("||", "|", _fstack)%;\
            /let _del_return=*-- Deletion successful --*%;\
        /endif%;\
    /done%;\
    /if (_del_return =~ '') \
        /echo -waetolia *-- Couldn't find %{_fc_rm_string}. --*%;\
    /else \
        /echo -waetolia %{_del_return}%;\
    /endif

/setenv _fc_delay=

/def _exec_cache = \
    /if (_fc_delay =~ '') \
        /if (_cache_active == 1) \
            /set _cache_active=0%;\
            /repeat -P 1 /set _cache_active=1%;\
        /endif%;\
        /if (_fstack =~ '') /echo -waetolia *-- Nothing to execute
--*%;/endif%;\
        /test regmatch("\\|([^\\|]*)", _fstack)%;\
        /let _fc_cmd=%{P1}%;\
        /if (_fc_cmd =~ '') \
            /setenv _fstack=%;\
            /echo -waetolia *-- Either there's been an error, or the
cache is empty. --*%;\
            /return%;\
        /endif%;\
        /echo -waetolia *-- Executing next stack command. --*%;\
        /repeat -S 1 /eval %{_fc_cmd}%;\
        /setenv _fstack=%{PR}%;\
    /endif

/def _print_cache = \
    /let _c_output_tmp=%{_fstack}%;\
    /let _c_last_output=%;\
    /let _c_cur_cmd_c=1%;\
    /let _searching=1%;\
    /if (_c_output_tmp =~ '') \
        /_echo *-- Cache is empty. --*%;\
    /else \
        /echo -waetolia %{_fstack}%;\
    /endif

/def _cache_help = \
    /quote -0 -decho '"~/aetolia/cache.help"

;; Cache command.  Modify this to use the rest of the cache.  Modify the
previous functions at
;; your own risk!  They are all vital to eachother to ensure the cache
runs smoothly.

/setenv _cache_active=

/def Cache = \
    /test getopts("ha:d:n#csrepx", "")%;\
    /let _c_add=$[escape("%'\"", opt_a)]%;\
    /let _c_rem=%{opt_d}%;\
    /let _c_anum=%{opt_n}%;\
    /let _c_clear=%{opt_c}%;\
    /let _c_help=%{opt_c}%;\
    /let _c_halt=%{opt_s}%;\
    /let _c_res=%{opt_r}%;\
    /let _c_exec=%{opt_e}%;\
    /let _c_print=%{opt_p}%;\
    /let _c_active=%{opt_x}%;\
\
    /if (_c_add !~ '') \
        /if (_c_anum > 0) \
            /echo -waetolia *-- Adding %{_c_anum} instances of
%{_c_add}. --*%;\
            /repeat -S %{_c_anum} /_add_cache %{_c_add}%;\
            /return%;\
        /else \
            /repeat -S 1 /_add_cache %{_c_add}%;\
            /return%;\
        /endif%;\
    /endif%;\
    /if (_c_rem !~ '') \
        /repeat -S 1 /_rm_cache %{_c_rem}%;\
        /return%;\
    /endif%;\
    /if (_c_clear > 0) \
        /repeat -S 1 /setenv _fstack=%;\
        /return%;\
    /endif%;\
    /if (_c_halt > 0) \
        /repeat -S 1 /_halt_cache%;\
        /return%;\
    /endif%;\
    /if (_c_res > 0) \
        /repeat -S 1 /_res_cache%;\
        /return%;\
    /endif%;\
    /if (_c_exec > 0) \
        /repeat -S 1 /_exec_cache%;\
        /return%;\
    /endif%;\
    /if (_c_print > 0) \
        /repeat -S 1 /_print_cache%;\
        /return%;\
    /endif%;\
    /if (_c_active > 0) \
        /if (_cache_active) \
            /toggle _cache_active%;\
            /if (_cache_active == 1) \
                /echo -waetolia *-- Cache is ACTIVE. --*%;\
            /else \
                /echo -waetolia *-- Cache is INACTIVE. --*%;\
            /endif%;\
            /return%;\
        /else \
            /set _cache_active=1%;\
            /echo -waetolia *-- Cache is ACTIVE. --*%;\
            /return%;\
        /endif%;\
    /endif%;\
    /_cache_help





