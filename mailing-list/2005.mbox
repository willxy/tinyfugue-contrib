;========================================================================
; size_array()
;
; Returns the total size of an array created with put_array()/put_2array().
; USAGE:
; x := size_array("array_name")
;
; x            : Total number of elements found in the array.
; "array_name" : Name of the array you want to get the size of.
;========================================================================
/def size_array = \
  /let size_ 0 %;\
  /def size_array2 = \
    /test ++size_ %;\
  /quote -S /test size_array2("`"/listvar -mglob -s _array_%1_*"") %;\
  /undef size_array2 %;\
  /RETURN size_


