-- is the terminal running?
set theResult to do shell script "ps -x | grep Terminal"
if theResult contains "Terminal.app" then
	set isTerminalOpen to true
else
	set isTerminalOpen to false
end if

tell application "Terminal"
	
	-- open the window
	set i to count of windows
	if isTerminalOpen is equal to true then
		do script "/Library/TinyFugue/bin/tf -n"
	else
		set i to count of windows
		set tfWindow to item i of windows
		do script "/Library/TinyFugue/bin/tf -n" in tfWindow
	end if
	
	activate
end tell


