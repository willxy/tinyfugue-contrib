/def ?array = /array_help

/def array_help = \
     /echo -p @{Cgreen}+$[strrep("-", 78)]+ %; \
     /test podziel_linie("Arrays lib for Tiny Fugue,  by volus", 78)%; \
     /test podziel_linie("Version  1.0", 78)%; \
     /echo -p @{Cgreen}|$[strrep(" ", 78)]| %; \
     /echo -p @{Cgreen}|$[strrep(" ", 78)]| %; \
     /echo -p @{Cgreen}|  * /add_array name index1 index2 index3 index4 
...                           |%; \
     /echo -p @{Cgreen}|         Makes an array named 'name' with 
indices 'index1 index2' etc.        |%; \
     /echo -p @{Cgreen}|$[strrep(" ", 78)]| %; \
     /echo -p @{Cgreen}|  * /add_index name index1 index2 index3 index4 
...                           |%; \
     /echo -p @{Cgreen}|         Adds 'indices' to array named 'name' 
                               |%; \
     /echo -p @{Cgreen}|$[strrep(" ", 78)]| %; \
     /echo -p @{Cgreen}|  * /sizeof_array name 
                               |%; \
     /echo -p @{Cgreen}|         Returns size of an array named 'name' 
                               |%; \
     /echo -p @{Cgreen}|$[strrep(" ", 78)]| %; \
     /echo -p @{Cgreen}|  * /check_index name number_of_index 
                               |%; \
     /echo -p @{Cgreen}|         Returns value of number_of_index in 
array named 'name'               |%; \
     /echo -p @{Cgreen}|$[strrep(" ", 78)]| %; \
     /echo -p @{Cgreen}+$[strrep("-", 78)]+

/def if_array_exists = \
     /if ($(/listvar -s _array_%{*})) \
         /result 1%; \
     /endif

/def add_array = \
     /if ({#} < 2) \
         /echo You must specify name of an array, and at least one 
index. %; \
         /echo For example: /add_array team john mark frank%; \
         /return %; \
     /endif %; \
     /if (regmatch("[()|]", {*})) \
         /echo Name of an array and indices can't have signs '(', ')', 
and '|'. %; \
         /echo Warning! 1 index is 1 word, number or letter %; \
         /echo So space is like end of an index. %; \
         /return %; \
     /endif %; \
     /set _array_%{1}=$[replace(" ", "|", {-1})]|

/def add_index = \
     /if ({#} < 2) \
         /echo You must specify the name of an array, and at least one 
index. %; \
         /echo For example: /add_index team chris %; \
         /echo Will add 'chris' into 'team' array%; \
         /return %; \
     /endif %; \
     /if (regmatch("[()|]", {*})) \
         /echo Name of an array and indices can't have signs '(', ')', 
and '|'. %; \
         /return %; \
     /endif %; \
     /if (if_array_exists({1})) \
         /set _array_%{1}=$(/eval /echo %%_array_%{1})$[replace(" ", 
"|", {-1})]|%;\
     /else \
         /echo Array with name '%{1}' does not exist!%; \
     /endif

/def sizeof_array = \
     /if ({#} < 1) \
         /echo You must specify the name of an array!%; \
         /echo For example: /sizeof_array team%; \
         /return %; \
     /endif %; \
     /if (if_array_exists({1})) \
         /let PomocniczaZmienna=$(/eval /echo %%_array_%{1})%; \
         /let tab=$[replace("|", " ", {PomocniczaZmienna})]%; \
         /let wynik=$(/policz_tablice %{tab})%; \
         /echo Array '%{1}' has %{wynik} indices. %; \
     /else \
         /echo There is no array with name '%{1}'. %; \
     /endif

/def policz_tablice = \
     /let k_z=0%; \
     /while ({#}) \
         /test ++k_z%; \
         /shift %; \
     /done %; \
     /result %{k_z}

/def check_index = \
     /if ({#} != 2) \
         /echo You must specify the name of an array, and number of 
index, which you want to check. %; \
         /echo If you choose index '0', I will show all indices. %; \
         /return %; \
     /endif %; \
     /if (if_array_exists({1})) \
         /let rozmiar_tablicy=$(/policz_tablice $[replace("|", " ", 
$(/eval /echo %%_array_%{1}))])%; \
         /if ({2} < 0) \
             /echo Index must be grater than 0. If you want me to show 
you all %; \
             /echo Indexes, type '0' as second argument%; \
             /return %; \
         /elseif ({2} != 0 & {2} > {rozmiar_tablicy}) \
             /echo Index is too large! This array has %{rozmiar_tablicy} 
elements!%; \
             /return %; \
         /endif%; \
         /if ({2} == 0) \
             /pokaz_wszystkie_indeksy_tablicy $[replace("|", " ", 
$(/eval /echo %%_array_%{1}))]%; \
         /else \
             /pokaz_indeks %{2} %{1} $[replace("|", " ", $(/eval /echo 
%%_array_%{1}))]%; \
         /endif %; \
     /else \
         /echo There is no array with name '%{1}'!%; \
     /endif

/def pokaz_wszystkie_indeksy_tablicy = \
     /let i_t=0%; \
     /while ({#}) \
         /test ++i_t%; \
         /echo %{i_t}: %{1}%; \
         /shift %; \
     /done

/def pokaz_indeks = \
     /let _nr=%{1}%; \
     /let _ta=%{2}%; \
     /shift %{1}%; \
     /shift %; \
     /echo Index %{_nr} of an array '%{_ta}' is '%{1}'
