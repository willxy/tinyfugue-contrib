;##############################;
;#####  logging controls  #####;
;##############################;

; globallog   - (plain text) captures mud and local tf output, as well as all keyboard input
; worldlog    - (plain text) captures mud output, and input sent to mud
; combatlog   - (ansi color) captures non-gagged mud output, anything extra I log (echoes), as well as input sent to mud

;### sets up the logs and begins logging everything
;### syntax: /start_logging
/def start_logging = \
    /echo -aBCcyan <<<<============        STARTING LOGS        ============>>>> %; \
    /eval /set globallog_filename=~/.tf/logs/$[ftime('%Y-%m-%d')].globallog %; \
    /eval /set worldlog_filename=~/.tf/logs/$[ftime('%Y-%m-%d')].worldlog %; \
    /eval /set combatlog_filename=~/.tf/logs/$[ftime('%Y-%m-%d')].combatlog %; \
    /log -g %{globallog_filename} %; \
    /log -i %{globallog_filename} %; \
    /log -w %{worldlog_filename} %; \
    /eval /set globallog=$[tfopen({globallog_filename},'a')] %; \
    /eval /set worldlog=$[tfopen({worldlog_filename},'a')] %; \
    /eval /set combatlog=$[tfopen({combatlog_filename},'a')] %; \
    /def -mregexp -n0 -p50 -q -t".*" catch_all_combatlog = \
        /test tfwrite({combatlog},encode_ansi({P0})) %; \
    /def -mregexp -n0 -p50 -F -q -h"SEND .*" catch_all_send = \
        /test tfwrite({worldlog},strcat('SENT: ',{P0})) %%; \
        /test tfwrite({combatlog},strcat('SENT: ',{P0})) %; \
    /def -mregexp -n0 -p50 -q -h"PROMPT (\\d+)h, (\\d+)m, (\\d+)e, (\\d+)p, (\\d+)en, (\\d+)w (.*)\\-" catch_all_prompt =  \
        /test tfwrite({globallog},{P0}) %%; \
        /test tfwrite({worldlog},{P0}) %%; \
        /test tfwrite({combatlog},encode_ansi({P0})) %; \
    /echol BCcyan 0  <<<<========== LOG STARTED $[ftime('%Y-%m-%d')] $[ftime('%H:%M:%S')] ==========>>>> %; \
    /send date

;### ceases logging everything and removes the tf resources for the logs
;### syntax: /stop_logging
/def stop_logging = \
    /echo -aBCcyan <<<<============        STOPPING LOGS        ============>>>> %; \
    /echol BCcyan 0  <<<<========== LOG STOPPED $[ftime('%Y-%m-%d')] $[ftime('%H:%M:%S')] ==========>>>> %; \
    /log -g off %; \
    /log -i off %; \
    /log -w off %; \
    /undef catch_all_combatlog %; \
    /undef catch_all_send %; \
    /undef catch_all_prompt %; \
    /test tfclose(globallog) %; \
    /test tfclose(worldlog) %; \
    /test tfclose(combatlog) %; \
    /unset globallog %; \
    /unset worldlog %; \
    /unset combatlog %; \
    /unset globallog_filename %; \
    /unset worldlog_filename %; \
    /unset combatlog_filename

;### stops the current logs and begins a new one
;### syntax: /restart_logging
/alias restart_logging \
    /stop_logging %; \
    /start_logging

;### allows you to write a string into the combatlog (designed for use with gag/replacements for the statusbar since the statusbar isn't available in logs)
;### syntax: /combatlog_entry (<attributes without the '-a'> | none) <text to echo>
/def combatlog_entry = \
    /if ({1} !~ "none") \
        /let texttolog=$(/test echo\({-1},{1}\)) %; \
        /test tfwrite({combatlog},encode_ansi({texttolog})) %; \
    /else \
        /let texttolog=$(/test echo\({*}\)) %; \
        /test tfwrite({combatlog},encode_ansi({texttolog})) %; \
    /endif


;################################;
;#####  macro replacements  #####;
;################################;


;### /echo replacement to allow echoes to be logged as well
;### syntax: /echol (<attributes without the '-a'> | none) <inline flag> <text to echo>
/def echol = \
    /if ({1} !~ "none") \
        /test echo({-2},{1},{2}) %; \
        /let texttolog=$(/test echo\({-2},{1},{2}\)) %; \
        /test tfwrite({combatlog},encode_ansi({texttolog})) %; \
    /else \
        /test echo({-2},'',{2}) %; \
        /let texttolog=$(/test echo\({-2},'',{2}\)) %; \
        /test tfwrite({combatlog},encode_ansi({texttolog})) %; \
    /endif

;### /send replacement to allow sent commands to be logged but not trigger the my_alias.tf SEND hook
;### syntax: /sendl <text to send>
/def sendl = \
    /send %{*} %; \
    /test tfwrite({worldlog},strcat('SENT: ',{*})) %; \
    /test tfwrite({combatlog},strcat('SENT: ',{*}))


