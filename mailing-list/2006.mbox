  /elseif ({arg} =~ "rel.uninj") /echo 8%; \
  /elseif ({arg} =~ "bruises++") /echo 7++%; \
  /elseif ({arg} =~ "bruises+") /echo 7+%; \
  /elseif ({arg} =~ "bruises") /echo 7%; \
  /elseif ({arg} =~ "lt.wnds++") /echo 6++%; \
  /elseif ({arg} =~ "lt.wnds+") /echo 6+%; \
  /elseif ({arg} =~ "lt.wnds") /echo 6%; \
  /elseif ({arg} =~ "gashes++") /echo 5++%; \
  /elseif ({arg} =~ "gashes+") /echo 5+%; \
  /elseif ({arg} =~ "gashes") /echo 5%; \
  /elseif ({arg} =~ "ser.wnds++") /echo 4++%; \
  /elseif ({arg} =~ "ser.wnds+") /echo 4+%; \
  /elseif ({arg} =~ "ser.wnds") /echo 4%; \
  /elseif ({arg} =~ "severe++") /echo 3++%; \
  /elseif ({arg} =~ "severe+") /echo 3+%; \
  /elseif ({arg} =~ "severe") /echo 3%; \
  /elseif ({arg} =~ "grievous++") /echo 2++%; \
  /elseif ({arg} =~ "grievous+") /echo 2+%; \
  /elseif ({arg} =~ "grievous") /echo 2%; \
