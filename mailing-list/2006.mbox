    /def -n1 -wos -aBCwhite -t'^(.+) shimmering images appear around you.'
detect_image = \
      /set images $$(/image_lookup %%{P1})%;\
    /def -wos -aBCcyan -t'shatters one of your mirror images.|mirror image of
you flickers and dies' detect_shatter = \
