  /endif

/def move_lookup = \
  /let arg=%* %; \
  /if ({arg} =~ "hyper") /echo 7%; \
  /elseif ({arg} =~ "energetic++") /echo 6++%; \
  /elseif ({arg} =~ "energetic+") /echo 6+%; \
