/require textutil.tf

/def dolog = \
    /let file=$(/quote -S -decho !"dolog.sh ${world_name} ${world_character}")%;\
    /let out=%; \
    /if ((out:=tfopen({file}, "a")) < 0) /return 0%; /endif%; \
    /echo -e %% Recalling to log file %{file}%;\
    /test tfflush(out, 0)%; \
    /quote -S -decho #%{1-0} %| /copyio i %out%; \
    /test tfclose(out)%; \
    /log %{file}

