  Have done it myself, here's my scripts..

---------------
;  Coloring code made by jaw@cd.chalmers.se
;

/set my_hp=?
/set my_hp_max=?
/set my_gp=?
/set my_gp_max=?
/set my_xp=?

; White until we know what it should be..
/set my_hp_color=white
/set my_gp_color=white


; Trigger of the displaying of the values and remember to call /do_status_colors after you
; have saved the values to the variables..
/def -F -i -mregexp -t"^Hp: ([0-9]+)\(([0-9]+)\)  Gp: ([0-9]+)\(([0-9]+)\)  Xp: ([0-9]+)" loc_trig_sb = \
        /set my_hp=%P1 %; \
        /set my_hp_max=%P2 %; \
        /set my_gp=%P3 %; \
        /set my_gp_max=%P4 %; \
        /set my_xp=%P5 %; \
        /do_status_colors

/def -F -i -mregexp -t"^Hp: ([0-9]+) \(([0-9]+)\) Gp: ([0-9]+) \(([0-9]+)\)" loc_trig_hpinfight = \
        /set my_hp=%P1 %; \
        /set my_hp_max=%P2 %; \
        /set my_gp=%P3 %; \
        /set my_gp_max=%P4 %; \
        /do_status_colors

/def -F -i -mregexp -t"^You have ([0-9]+) \(([0-9]+)\) guild points\." loc_trig_gp = \
      /set my_gp=%P1 %; \
      /set my_gp_max=%P2 %; \
      /do_status_colors

; Calculate the colors..
/def -i do_status_colors = \
        /if ( my_hp > (my_hp_max / 2) ) \
                /set my_hp_color=green %; \
        /elseif ( my_hp < (my_hp_max / 4) ) \
                /set my_hp_color=red %; \
        /else %; \
                /set my_hp_color=yellow %; \
        /endif %; \
        /if ( my_gp > (my_gp_max / 2) ) \
                /set my_gp_color=green %; \
        /elseif ( my_gp < (my_gp_max / 4) ) \
                /set my_gp_color=red %; \
        /else %; \
                /set my_gp_color=yellow %; \
        /endif %; \
        /set_status_bar

; This is how we set the bar..
/def -i set_status_bar = \
        /set status_fields="MyName" :2 "Hp:" my_hp:4:C%{my_hp_color} "(" my_hp_max:4 \
        ")" :2 "Gp:" my_gp:3:C%{my_gp_color} "(" my_gp_max:3 ")" :2 "Xp:" my_xp:6 \
        @log:5 :1 insert :1 @clock:5

; Set it the first time also..
/set_status_bar

--------------------

