;;; $Id: lists.txt,v 1.1 1998/03/03 22:32:11 dittmar Exp dittmar $
;;;
;;; coded by Susan Dittmar
;;; (dittmar dot ph dash cip dot uni dash koeln dot de), October 1997,
;;; bigger changes January 1998

;;; I'll describe the usage of this little tinyfugue-library with the
;;; syntax of help files. This way, if you use your own help texts with
;;; tinyfugue (like with the /help command by Johan van Selst), you can
;;; just copy this part to your help file, you just have to remove the
;;; semicolons.

;;; The library requires tinyfugue Version 4.0 for the file handling part,
;;; the rest of the code will work with earlier versions too.

;;; &lists
;;;
;;; I wrote these commands to make some triggers much easier.
;;; The commands only keep track of variables which contain items
;;; separated by '|'. These variables (lists) then can be used for
;;; global matching, like
;;;    /if /test {variable} =3D/ "{%{list}}"=A0%; /then ...
;;;
;;; For each list there is a variable with the name <list>__count which
;;; counts the number of items in the list. If you only add and remove
;;; single items, this number will be up to date, else it can be computed.
;;;
;;; the commands are
;;; /addlist <list> <item>    adds <item> to the list if it does not exist
;;;                           yet
;;; /addlist <list>           echos the whole list
;;; addlist <arguments>       executes /addlist with it's arguments and
;;;                           writes the necessary commands to reproduce the
;;;                           list to the file you specified
;;; /remlist <list> <item>    removes <item> from the list if it exists.
;;; /remlist <list> -a        clears the whole list
;;; remlist <arguments>       executes /remlist with it's arguments and
;;;                           writes the necessary commands to reproduce the
;;;                           list to the file you specified
;;; /countlist <list>         counts the number of items in the list
;;; /changeListFile <filename>    changes the file for saving lists to a
;;;                           file called <filename>. In case such a file
;;;                           already exists, the new things will be append=
ed,
;;;                           nothing in the file will be lost.
;;; /foreach <variable> <list name> <command>    executes a loop. In each
;;;                           turn one item of the list is assigned to the
;;;                           variable <variable>, then the command <comman=
d>
;;;                           (this can be a list of commands separated by=
=A0%;
;;;                           if you need) is executed with this value of
;;;                           the variable <variable>
;;;
;;; #change
;;; #changes
;;; 5.1.1998:
;;;   * added the counter <list>__count
;;;   * added /countlist
;;; 22.1.1998:
;;;   * added writing to a file (addlist, remlist, /changeListFile)
;;;   * added command /foreach
;;; 28.1.1998:
;;;   * added the list __LIST, which contains the name of all lists
;;;     which were used
;;; 29.1.1998:
;;;   * added /sortlist <list> command
;;;   * added /saveAllLists command (sorts and then saves all lists stored
;;;     in __LIST)
;;;
;;; #author
;;; #bug
;;; #bugs
;;; #typo
;;; #typos
;;; #suggestion
;;; #suggestions
;;; The commands have been written by Susan Dittmar
;;; An actual version can be obtained from
;;; http://www.ph-cip.uni-koeln.de/~dittmar/tf. Please send bug reports
;;; and suggestions to the author.
;;;

;;; You can change the next line to /~loaded lists.tf if you want.
/~loadedlocal lists.tf
/require alias.tf
/requirelocal array.tf

/def addlist =3D \
    /let list=3D%1%;\
    /if (list =3D~ "") /echo=A0%%% you must provide the list name=A0%; /end=
if=A0%;\
    /let item=3D%-1%;\
    /if ({list}=A0!~ "__LIST") \
        /if /test {list}=A0!/ "%{__LIST}"=A0%; /then \
            /addlist __LIST=A0%{list}=A0%;\
        /endif=A0%;\
    /endif=A0%;\
    /if (item =3D~ "") \
        /eval /echo \% List=A0%{list} contains=A0%%{%{list}}%;\
;        /eval /echo /addlist=A0%{list}=A0%%{%{list}}%;\
        /break%;\
    /endif%;\
;    /eval /eval /echo item=3D%{item}, list=3D%%{%{list}}=A0%;\
    /eval /eval /if (item =3D/ "{%%{%{list}}}") \
        /break%%%;\
    /elseif (%{list} =3D~ "") \
        /set=A0%{list}=3D%{item}%%%;\
        /set=A0%{list}__count=3D1%%%;\
    /else \
        /set=A0%{list}=3D%%{%{list}}|%{item}%%%;\
        /set=A0%{list}__count=3D$$$[%%{%{list}__count}+1]%%%;\
    /endif



/def remlist =3D\
    /let list=3D%1%;\
    /if (list =3D~ "") \
        /echo=A0%%% you must provide the list name=A0%;\
        /break=A0%;\
    /endif=A0%;\
    /let item=3D%-1%;\
    /if (item =3D~ "") \
        /echo \% Use /remlist <list> <item> or /remlist <list> -a for all.%=
;\
        /break%;\
    /endif%;\
    /eval /eval /if ((item =3D~ "-a")|(item =3D~ "%%{%{list}}")) \
        /set=A0%{list}=3D%%%;\
        /set=A0%{list}__count=3D0%%%;\
    /elseif (item =3D/ "{%%{%{list}}}") \
        /set=A0%{list}=3D$$$(/_lists_remove=A0%%%{item}|=A0%%%{%%{list}})%%=
%;\
        /set=A0%{list}=3D$$$(/_lists_remove |%%%{item}=A0%%%{%%{list}})%%%;\
        /set=A0%{list}__count=3D$$$[%%{%{list}__count}-1]%%%;\
    /endif

/def _lists_remove =3D \
    /let old=3D%1%;\
    /let left=3D%;\
    /let right=3D%-1%;\
    /while /let i=3D$[strstr(right, old)]%; /test i >=3D 0%; /do \
         /test left=A0:=3D strcat(left, substr(right, 0, i), new)%;\
         /test right=A0:=3D substr(right, i + strlen(old))%;\
    /done%;\
    /echo -=A0%{left}%{right}


/def countlist =3D\
    /let list=3D%1%;\
    /if (list =3D~ "") \
        /echo=A0%%% you must provide the list name=A0%;\
        /break=A0%;\
    /endif=A0%;\
    /eval /if ({%{list}} =3D~ "") \
            /set=A0%{list}__count=3D0%%;\
            /break=A0%%;\
        /else \
            /set=A0%{list}__count=3D1%%;\
        /endif=A0%;\
    /eval /let right=3D%%{%{list}}%%;\
        /while /let i=3D$$[strchr(right, "|")]%%; /test i >=3D 0%%; /do \
             /test right=A0:=3D substr(right, i + 1)%%;\
             /eval /set=A0%{list}__count=3D$$$[%%{%{list}__count}+1]%%;\
        /done


/def sortlist =3D\
    /let list=3D%1%;\
    /if (list =3D~ "") \
        /echo=A0%%% you must provide the list name=A0%;\
        /break=A0%;\
    /endif=A0%;\
    /countlist=A0%{list}%;\
    /let countmax=3D$(/eval /echo -=A0%%{%{list}__count})%;\
    /foreach j=A0%{list} /append_array _list=A0%%{j}=A0%;\
    /sort_array _list=A0%;\
    /remlist=A0%{list} -a=A0%;\
    /let j=3D1=A0%;\
    /while (j<=3Dcountmax) \
        /eval /addlist=A0%{list}=A0%%{_list__%{j}}=A0%;\
        /test ++j=A0%;\
    /done=A0%;\
    /clear_array _list


/def changeListFile =3D \
    /if ({#}=A0!=3D 1) \
        /echo -e=A0%%% usage: /changeListFile <filename>=A0%;\
        /break=A0%;\
    /endif=A0%;\
    /test tfclose(_filehandle_lists)=A0%;\
    /set _filehandle_lists=3D$[tfopen({1},"a")]


/alias addlist \
    /if ({#} =3D 1) \
        /test tfwrite({_filehandle_lists},strcat("/set ",{1},"=3D",{%{1}}))=
=A0%;\
    /elseif ({#} > 1) \
        /test tfwrite({_filehandle_lists},strcat("/addlist ",{*}))=A0%;\
    /endif=A0%;\
    /addlist=A0%{*}

/alias remlist \
    /if ({#} >=3D 2) \
        /test tfwrite({_filehandle_lists},strcat("/remlist ",{*}))=A0%;\
    /endif=A0%;\
    /remlist=A0%{*}



/def foreach =3D \
    /if ({#} < 3) \
        /echo -e=A0%%% usage: /foreach <variable> <list> <command>=A0%;\
        /break=A0%;\
    /endif=A0%;\
    /if ({1} =3D~ "__i") \
        /echo -e=A0%%% /foreach: don't use __i as variable=A0%;\
        /break=A0%;\
    /endif=A0%;\
    /let variable=3D%{1}%;\
    /let list=3D$(/eval /echo -=A0%%{%{2}})%;\
    /let command=3D%{-2}%;\
    /while ({list}=A0!~ "") \
        /let __i=3D$[strstr({list},"|")]=A0%;\
        /if (__i<0) \
            /let=A0%{variable}=3D%{list}=A0%;\
            /let list=3D%;\
        /else \
            /let=A0%{variable}=3D$[substr({list},0,__i)]=A0%;\
            /let list=3D$[substr({list},__i+1)]=A0%;\
        /endif=A0%;\
        /eval -s0=A0%{command}=A0%;\
    /done



/def saveAllLists =3D \
    /sortlist __LIST=A0%;\
    addlist __LIST=A0%;\
    /foreach i __LIST /eval /sortlist=A0%%{i}=A0%%; addlist=A0%%{i}=A0%;\

