/def vibrations_tick = \

/if ({vibrations_time} <= 3) \

; /send ka tahto'sho ground %; \

/set vibrations_time=0 %; \

/set vibrations_timer_on=0 %; \

/echol BCCyan 0 Vibrations Ending ->>> %; \

/elseif ({vibrations_time} <= 60) \

/vibrations_countdown %; \

/set vibrations_time=$[{vibrations_time}-1] %; \

/repeat -1 1 /vibrations_tick %; \

/endif



;### /vibrationss_tick (echos the countdown)

/def vibrations_countdown = \

/echo -A -aBCcyan [ %{vibrations_time} ] Vibrations

