before, but wanted to rewrite it and get a more syncronized timer.

/set bomb_timeset=10
# explodes around ~12
/set bomb_time=0

/def bomb_tick = \
/if ({bomb_time} == {bomb_timeset}) \
/bomb_throw %; \
/set bomb_time=0 %; \
/else \
/set bomb_time=$[{bomb_time}+1] %; \
/bomb_countdown %; \
/repeat -1 1 /bomb_tick %; \
/endif

/def bomb_countdown = \
/echo -aBCred BOMBTIME: [ %{bomb_time} ]

/def bomb_throw = \
# /set spam=5 %; \
