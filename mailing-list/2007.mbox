+
+    ---------------------------------------------
+    -- Return indentation string for passed level
+    ---------------------------------------------
+    local function tabs(i)
+        return string.rep(".",i).." "   -- Dots followed by a space
+    end
+
+    -----------------------------------------------------------
+    -- Return string representation of parameter's value & type
+    -----------------------------------------------------------
+    local function toStrType(t)
+        local function fttu2hex(t) -- Grab hex value from tostring() output
+            local str =3D tostring(t);
+            if str =3D=3D nil then
+                return "tostring() failure! \n"
+            else
+                local str2 =3D string.match(str,"[ :][ (](%x+)")
+                if str2 =3D=3D nil then
+                    return "string.match() failure: "..str.."\n"
+                else
+                    return "0x"..str2
+                end
+            end
+        end
+        -- Stringify a value of a given type using a table of functions ke=
yed
+        -- by the name of the type (Lua's version of C's switch() statemen=
t).
+        local stringify =3D {
+            -- Keys are all possible strings that type() may return,
+            -- per http://www.lua.org/manual/5.1/manual.html#pdf-type.
+            ["nil"]			=3D function(v) return "nil (nil)"			    end,
+            ["string"]		=3D function(v) return '"'..v..'" (string)'	    en=
d,
+            ["number"]		=3D function(v) return v.." (number)"			    end,
+            ["boolean"]		=3D function(v) return tostring(v).." (boolean)" =
 end,
+            ["function"]	=3D function(v) return fttu2hex(v).." (function)"=
 end,
+            ["table"]		=3D function(v) return fttu2hex(v).." (table)"	end,
+            ["thread"]		=3D function(v) return fttu2hex(v).." (thread)"	en=
d,
+            ["userdata"]	=3D function(v) return fttu2hex(v).." (userdata)"=
 end
+        }
+        return stringify[type(t)](t)
+    end
+
+    -------------------------------------
+    -- Count elements in the passed table
+    -------------------------------------
+    local function lenTable(t)		-- What Lua builtin does this simple thing?
+        local n=3D0                   -- '#' doesn't work with mixed key t=
ypes
+        if ("table" =3D=3D type(t)) then
+            for key in pairs(t) do  -- Just count 'em
+                n =3D n + 1
+            end
+            return n
+        else
+            return nil
+        end
+    end
+
+    --------------------------------
+    -- Pretty-print the passed table
+    --------------------------------
+    local function do_dumptable(t, indent, seen)
+        -- "seen" is an initially empty table used to track all tables
+        -- that have been dumped so far.  No table is dumped twice.
+        -- This also keeps the code from following self-referential loops,
+        -- the need for which was found when first dumping "_G".
+		local ret =3D ""
+        if ("table" =3D=3D type(t)) then	-- Dump passed table
+            seen[t] =3D 1
+            if (indent =3D=3D 0) then
+                ret =3D ret .. "The passed table has "..lenTable(t).." ent=
ries:"
+                indent =3D 1
+            end
+            for f,v in pairs(t) do
+                if ("table" =3D=3D type(v)) and (seen[v] =3D=3D nil) then =
   -- Recurse
+                    ret =3D ret ..  tabs(indent)..toStrType(f).." has "..l=
enTable(v).." entries: {"
+                    do_dumptable(v, indent+1, seen)
+                    ret =3D ret .. tabs(indent).."}"=20
+                else
+                    ret =3D ret .. tabs(indent)..toStrType(f).." =3D "..to=
StrType(v)
+                end
+            end
+        else
+            ret =3D ret .. tabs(indent).."Not a table!"
+        end
+
+		return ret
+    end
+
+    --------------------------------
+    -- Wrapper to handle persistence
+    --------------------------------
+    function dumptable(t)   -- Only global declaration in the package
+        -- This wrapper exists only to set the environment for the first r=
un:
+        -- The second param is the indentation level.
+        -- The third param is the list of tables dumped during this call.
+        -- Getting this list allocated and freed was a pain, and this
+        -- wrapper was the best solution I came up with...
+        return do_dumptable(t, 0, {})
+    end
