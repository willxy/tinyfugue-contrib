   /echo We have bought stuff%;\
  /else \
    /set clangold_diff=$[{clangold_current}-{clangold_old_count}]%;\
    /set clanqp_diff=$[{clanqp_current}-{clanqp_old_count}]%;\
    /3some clangold_diff%;\
