+++ tf5-debug/src/history.c	2003-10-15 17:47:57.000000000 +0200
@@ -152,7 +152,7 @@
 void recordline(History *hist, String *line)
 {
     if (!(line->attrs & F_NOHISTORY) && !norecord) save_to_hist(hist, line);
-    if (hist->logfile && !nolog) save_to_log(hist, line);
+    if (hist->logfile && !nolog && !(line->attrs & F_GAG)) save_to_log(hist, line); // Waba: /def -ag fix
 }
 
 static void hold_input(const String *instr)
diff -ur tf-50a11/src/output.c tf5-debug/src/output.c
--- tf-50a11/src/output.c	2003-05-28 08:55:51.000000000 +0200
+++ tf5-debug/src/output.c	2003-10-15 19:10:56.000000000 +0200
@@ -1490,7 +1490,8 @@
     ListEntry *node;
     StatusField *field;
     int right = 0;
-    int column, need_xy;
+    int need_xy = 0; // Waba: Valgrind warning #0
+    int column;
     attr_t attrs = 0;
 
     if (screen_mode < 1) return 0;
@@ -2688,7 +2689,8 @@
 	    *starting = current;
 	} else if (*starting != current) {
 	    /* First mid-line attr change. */
-	    check_charattrs(line, dest - line->data, *starting,
+	    check_charattrs(line, line->len, *starting, // Waba: Valgrind warning #1
+//	    check_charattrs(line, dest - line->data, *starting,
 		__FILE__, __LINE__);
 	}
     }
@@ -2841,8 +2843,9 @@
             if (off) attrs &= ~new;
             else attrs |= new;
 
-        } else if (is_print(*s)) {
-            set_attr(line, t, &starting_attrs, attrs);
+        } else if (is_print(*s) || *s == '\033') { // Waba: ESC stripping bug
+            set_attr(line, t, &starting_attrs, // Waba: /echo -p strips off previous attrs -bug
