; mygrep.tf by Fred Clift
; public domain code - use however you want
; but please be nice and keep my name  with it.


; command on server go generate start and stop string (replace asdf with yours)
/def grepboundary = /send asdf
; a glob matching the string generated by the command above
/set grepresponse *Pardon?*


/def grepon  = /def -p3 -mglob -t'*%{*}*' show %; /def -mglob -p2 -ag -t'*' hide
/def grepoff = /undef hide %; /undef show

/def mygrep = \
    /def -n1 -ag -t'%{grepresponse}' -p5 grepstart = /grepon %1 %; \
    /def -n1 -ag -t'%{grepresponse}' -p4 grepstop = /grepoff %; \
    /grepboundary %; %2 %; /grepboundary

