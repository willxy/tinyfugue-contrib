;;; Syntax: getpath(<pathname>)
;;; Returns the path for <pathname>.
;
/def getpath = \
	/if (strlen({1})) \
		/return $$(/expr \{$[strcat("gopath_",{1})]\}) %; \
	/else \
		/return %; \
	/endif

;;; Syntax: /go [-c<command>] <path>
;;; Moves char along path. If <path> begins with +, a presaved path will be used.
;;; <path> can be in either of two formats:
;;; 1) All exits 1 char long and in a string: nnwwwswdnnn
;;; 2) All exits separated by a non-alphanumeric char: n;n;w;n;n;w;d;d;open west;w
;;;
;;; If switch -c is given, it checks the string after. Anything following up
;;; to the space will be a command that is said before each exit.
;;; If no command is given, it defaults to {gocommand}, or "go" by default.
;;;
;;; Note: if the exits have to contain a non-alphanumeric/non-underscore/non-space
;;; character before the separation character then please put the separation
;;; character first. Such as: /n-w/n/n/n/e
;;; The reason for this is the code takes the first character that's not alphanumeric,
;;; an underscore, or a space, and saves it as the separation character. If no characters
;;; like this are in the string, it uses space. If no spaces are in the string, it
;;; evaluates every char as a separate exit to take, so you can use the string nnwwnw.
;;; All these path rules are the same for saved paths.
;;;
;;; Another note: This command can be used for more than exits. You can
;;; use it to save groups of commands too, not just exits paths.
;;; Example: /savepath mush ;cast 'magic mushroom';get mushroom;eat mushroom
;;; In that example, the ; was put at the beginning to define it as the
;;; separation character. Otherwise ' would have become the separation character.
;;; Just be warned, however, that when doing this, you need to specify
;
/def go = \
	/let opt_c %{gocommand-go} %; \
	/test getopts("c:") %; \
	/if ({#} == 0) \
		/_echo %% //go :  Please specify a path. %; \
		/break %; \
	/endif %; \
	/if (substr({*},0,1) =~ "+") \
		/if (strstr({*}," ") != -1) \
			/_echo %% //go :  Path names cannot contain spaces. %; \
			/break %; \
		/endif %; \
		/if (strlen(getpath(substr({*},1)))) \
			/let path $[getpath(substr({*},1))] %; \
		/else \
			/_echo %% //go :  Path \($[substr({*},1)]\) not defined. %; \
			/break %; \
		/endif %; \
	/else \
		/let path %{*} %; \
	/endif %; \
	/if (regmatch("[^a-zA-Z0-9_]",{path})) \
		/if (regmatch("[^a-zA-Z0-9_ ]",{path})) \
			/let i -1 %; \
			/while (i < (strlen({path}) - 1)) \
				/if (regmatch("[^a-zA-Z0-9_ ]",substr({path},(++i),1))) \
					/let sep $[ascii(substr({path},i,1))] %; \
					/exit %; \
				/endif %; \
			/done %; \
			/if (strlen({sep}) == 0) \
				/_echo %% //go : Error: %%\{sep\} not defined. %; \
				/break %; \
			/endif %; \
		/else \
			/let sep 32 %; \
		/endif %; \
		/let subpath %{path} %; \
		/let stop $[strstr({subpath},(char({sep})))] %; \
		/while (stop >= 0) \
			/if (stop == (strlen({subpath}) - 1)) \
				/let subpath $[substr({subpath},0,-1)] %; \
				/break %; \
			/endif %; \
			/if (stop != 0) \
				/let exit $[substr({subpath},0,stop)] %;\
				/send $[strlen(opt_c) == 0 ? "" : strcat({opt_c}," ")]%{exit} %; \
			/endif %; \
			/let subpath $[substr({subpath},(stop + 1))] %; \
			/let oldstop %{stop} %; \
			/let stop $[strstr({subpath},(char({sep})))] %; \
		/done %; \
		/let exit %{subpath} %; \
		/if (strlen({exit})) \
			/send $[strlen(opt_c) == 0 ? "" : strcat({opt_c}," ")]%{exit} %; \
		/endif %; \
	/else \
		/for i 0 $[strlen({path}) - 1] \
			/send $[strlen(opt_c) == 0 ? "" : strcat({opt_c}," ")]$$[substr({path},i,1)] %; \
	/endif %; \

;;; Syntax: /savepath <path>
;;; Saves paths for the /go command. Uses same path rules as /go.
;
/def savepath = \
	/if ({#} >= 2) \
		/if (strlen(getpath({1}))) \
			/_echo %% //savepath :  \
				Old path \(%{1}: $[getpath({1})]\) erased. %; \
		/endif %; \
		/set gopath_%{1} %{-1} %; \
		/if (strlen({savepath_list})) \
			/let str %{savepath_list} %; \
			/let stop $[strstr({str}," ")] %; \
			/let presaved 0 %; \
			/while (stop >= 0) \
				/let name $[substr({str},0,stop)] %; \
				/if (name =~ {1}) \
					/let presaved 1 %; \
					/exit %; \
				/endif %; \
				/let str $[substr({str},(stop + 1))] %; \
				/let stop $[strstr({str}," ")] %; \
			/done %; \
			/let name %{str} %; \
			/if (strlen({name}) & name =~ {1}) \
				/let presaved 1 %; \
			/endif %; \
		/else \
			/let presaved 0 %; \
		/endif %; \
		/if (! presaved) \
			/set savepath_list $[strcat({savepath_list},(strlen({savepath_list}) ? \
			" " : ""),{1})] %; \
		/endif %; \
		/_echo %% //savepath :  Path (%{1}: %{-1}) saved. %; \
	/else \
		/_echo %% //savepath :  Please specify a path$[strlen({1}) ? "" : "name"] %; \
	/endif

;;; Syntax: /savepaths [<file>]
;;; Stores saved paths to <file> if given. If not given, saves to {savepath_file},
;;; or tiny.paths by default.
;
/def savepaths = \
	/let filename $[strlen({1}) ? {1} : {savepath_file-~/tiny.paths}] %; \
	/let file $[tfopen({filename},"w")] %; \
	/_echo %% Writing path definitions to \
		$[substr({filename},0,1) =/ "{~|/}" ? {filename} : \
			strcat({HOME},(substr({HOME},-1) =~ "/" ? "" : "/"),{filename})] %; \
	/let str %{savepath_list} %; \
	/let stop $[strstr({str}," ")] %; \
	/while (stop >= 0) \
		/let line \
			$[strcat(substr({str},0,{stop})," ",(getpath(substr({str},0,{stop}))))] %; \
		/test tfwrite(file,line) %; \
		/let str $[substr({str},(stop + 1))] %; \
		/let stop $[strstr({str}," ")] %; \
	/done %; \
	/let line $[strcat({str}," ",(getpath({str})))] %; \
	/if (strlen({line})) \
		/test tfwrite(file,line) %; \
	/endif %; \
	/test tfclose(file) %; \

;;; Syntax: /loadpaths [<file>]
;;; Reads saved paths from <file> if given. If not given, reads from {savepath_file},
;;; or tiny.paths by default.
;
/def loadpaths = \
	/let filename $[strlen({1}) ? {1} : {savepath_file-~/tiny.paths}] %; \
	/if /ismacro savepath %; /then \
		/_echo %% Reading path definitions from \
			$[substr({filename},0,1) =/ "{~|/}" ? {filename} : \
			strcat({HOME},(substr({HOME},-1) =~ "/" ? "" : "/"),{filename})] %; \
	/else \
		/_echo %% //loadpaths :  Macro //savepath is not defined. %; \
		/break %; \
	/endif %; \
	/let file $[tfopen({filename},"r")] %; \
	/let line=initialize %; \
	/while (tfread(file,line) >= 0) \
		/savepath %{line} %; \
	/done %; \
	/test tfclose(file)

;;; Syntax: /listpaths
;;; Lists all saved paths.
;
/def listpaths = \
	/if (strlen({savepath_list})) \
		/echo %% Listing saved paths: %; \
		/let str %{savepath_list} %; \
		/let stop $[strstr({str}," ")] %; \
		/while (stop >= 0) \
			/_echo %% $[strcat(substr({str},0,{stop}),": ", \
				(getpath(substr({str},0,{stop}))))] %; \
			/let str $[substr({str},(stop + 1))] %; \
			/let stop $[strstr({str}," ")] %; \
		/done %; \
		/let line $[strcat({str},": ",(getpath({str})))] %; \
		/if (strlen({line})) \
			/_echo %% %{line} %; \
		/endif %; \
	/else \
		/_echo %% No saved paths. %; \
	/endif

;;; Syntax: /delpath <pathname>
;;; Deletes path <pathname>.
;
/def delpath = \
	/if (strlen({1})) \
		/if (strlen({savepath_list}) & strlen(getpath({1}))) \
			/let str %{savepath_list} %; \
			/let stop $[strstr({str}," ")] %; \
			/let newlist= %; \
			/while (stop >= 0) \
				/let name $[substr({str},0,stop)] %; \
				/if (name !~ {1}) \
					/let newlist \
						$[strcat({newlist},(strlen({newlist}) ? " " : ""),{name})] %; \
				/endif %; \
				/let str $[substr({str},(stop + 1))] %; \
				/let stop $[strstr({str}," ")] %; \
			/done %; \
			/let name %{str} %; \
			/if (strlen({name}) & name !~ {1}) \
				/let newlist $[strcat({newlist},(strlen({newlist}) ? " " : ""),{name})] %; \
			/endif %; \
			/if (strlen({newlist})) \
				/set savepath_list %{newlist} %; \
			/else \
				/unset savepath_list %; \
			/endif %; \
			/let temp $[getpath({1})] %; \
			/unset gopath_%{1} %; \
			/_echo %% //delpath :  Path \(%{1}: %{temp}\) deleted. %; \
		/else \
			/_echo %% //delpath :  Path \(%{1}\) does not exist. %; \
		/endif %; \
	/else \
		/_echo %% //delpath :  Please supply a pathname. %; \
	/endif

;;; Syntax: /copypath <pathname1> <pathname2>
;;; Copies saved path <pathname1> to <pathname2>
;
/def copypath = \
	/if (strlen({2})) \
		/if (strlen(getpath({1}))) \
			/savepath %{2} $[getpath({1})] %; \
		/else \
			/_echo %% //copypath :  Path \(%{1}\) does not exist. %;\
		/endif %; \
	/else \
		/_echo %% //copypath :  Please supply \
			$[strlen({1}) ? "second" : "first" ] pathname. %; \
	/endif

;;; Syntax: /movepath <pathname1> <pathname2>
;;; Moves saved path <pathname1> to <pathname2>
;
/def movepath = \
	/if (strlen({2})) \
		/if (strlen(getpath({1}))) \
			/savepath %{2} $[getpath({1})] %; \
			/delpath %{1} %; \
		/else \
			/_echo %% //movepath :  Path \(%{1}\) does not exist. %;\
		/endif %; \
	/else \
		/_echo %% //movepath :  Please supply \
			$[strlen({1}) ? "second" : "first" ] pathname. %; \
	/endif

